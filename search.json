---
layout: null
---
[
{% assign docs = site.pages | concat: site.posts %}
{% for doc in docs %}
  {
    "title": {{ doc.title | jsonify }},
    "url": "{{ doc.url }}",
    "content": {{ doc.content | strip_html | strip_newlines | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
